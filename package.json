{
  "name": "ng-esm",
  "version": "0.4.0",
  "description": "ES2015 modules and decorators with Angular 1",
  "main": "dist/main.js",
  "jsnext:main": "dist/esm/main.js",
  "typings": "dist/esm/main.d.ts",
  "files": [
    "dist",
    "example"
  ],
  "scripts": {
    "start": "run-p example:watch example:serve",
    "example:watch": "tsc -w -p example",
    "example:serve": "browser-sync start -s . -f example/**/*.js example/*.html --no-open",
    "build": "tsc -p . && tsc -d -p . -m es2015 --outDir dist/esm",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build"
  },
  "keywords": [
    "angular",
    "ES2015",
    "ES6",
    "decorators"
  ],
  "author": "Christian Svensson <csvn.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/csvn/ng-esm.git"
  },
  "dependencies": {},
  "devDependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-aria": "^1.5.7",
    "angular-material": "^1.1.0-rc.5",
    "angular-ui-router": "^0.3.1",
    "browser-sync": "^2.13.0",
    "ng-esm": "0.4.0",
    "npm-run-all": "^2.1.2",
    "rimraf": "^2.5.2",
    "rollup": "^0.32.0",
    "rollup-plugin-typescript": "^0.7.6",
    "rollup-watch": "^2.4.0",
    "systemjs": "^0.19.31",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10"
  }
}
