{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "_runner": "terminal",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "start",
      "args": ["start"]
    },
    {
      "taskName": "build",
      "args": ["run", "build"]
    },
    {
      "taskName": "install",
      "args": ["install"]
    },
    {
      "taskName": "watch",
      "args": ["run", "watch"]
    }
  ]
}
