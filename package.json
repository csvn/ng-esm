{
  "name": "ng-esm",
  "version": "2.0.0",
  "description": "ES2015 modules and decorators with AngularJS",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "^6.9.0",
    "npm": "^4.0.0"
  },
  "scripts": {
    "prestart": "rimraf example/dist",
    "start": "run-p watch:example serve",
    "serve": "browser-sync start -s . -f example/* example/**/*.js --startPath /example",
    "test": "echo \"No tests yet...\" && exit 0",
    "prebuild": "rimraf dist",
    "build": "tsc -d && tsc -m commonjs --outDir dist/cjs",
    "watch": "tsc -w -d",
    "watch:example": "webpack -w",
    "prepublish": "in-publish && npm run build || not-in-publish"
  },
  "keywords": [
    "angular",
    "ES2015",
    "ES6",
    "decorators"
  ],
  "author": "Christian Svensson <csvn.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/csvn/ng-esm.git"
  },
  "dependencies": {
    "tslib": "^1.5.0"
  },
  "peerDependencies": {
    "angular": "~1.5.11",
    "angular-ui-router": "^1.0.0-rc.1"
  },
  "devDependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/upgrade": "^2.4.8",
    "@types/angular": "~1.5.23",
    "@types/angular-material": "^1.1.45",
    "angular": "~1.5.11",
    "angular-animate": "~1.5.11",
    "angular-aria": "~1.5.11",
    "angular-material": "^1.1.3",
    "angular-ui-router": "^1.0.0-rc.1",
    "browser-sync": "^2.18.8",
    "core-js": "^2.4.1",
    "in-publish": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.2",
    "karma-typescript-preprocessor2": "^1.2.1",
    "npm-run-all": "^4.0.1",
    "rimraf": "^2.6.0",
    "rxjs": "^5.2.0",
    "source-map-loader": "^0.1.6",
    "ts-loader": "^2.0.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "zone.js": "^0.7.7"
  }
}
