{
  "name": "ng-esm",
  "version": "1.5.1",
  "description": "ES2015 modules and decorators with Angular 1",
  "main": "dist/main.js",
  "jsnext:main": "dist/esm/main.js",
  "typings": "dist/esm/main.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "^6.9.0",
    "npm": "^4.0.0"
  },
  "scripts": {
    "prestart": "node scripts/symlink.js",
    "start": "run-p watch:* serve",
    "serve": "browser-sync start -s . -f dist example/dist example/**/*.html example/**/*.css --startPath /example",
    "test": "echo \"No tests yet...\" && exit 0",
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:cjs": "tsc -p src -m commonjs --outDir dist",
    "build:esm": "tsc -d -p src",
    "watch:cjs": "tsc -w -p src -m commonjs --outDir dist",
    "watch:esm": "tsc -w -d -p src",
    "watch:example": "tsc -w -p example",
    "prepublish": "in-publish && npm run build || not-in-publish"
  },
  "keywords": [
    "angular",
    "ES2015",
    "ES6",
    "decorators"
  ],
  "author": "Christian Svensson <csvn.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/csvn/ng-esm.git"
  },
  "dependencies": {
    "tslib": "^1.5.0"
  },
  "peerDependencies": {
    "angular": "~1.5.11",
    "angular-ui-router": "^1.0.0-rc.1"
  },
  "devDependencies": {
    "@types/angular": "~1.5.23",
    "@types/angular-material": "^1.1.45",
    "angular": "~1.5.11",
    "angular-animate": "~1.5.11",
    "angular-aria": "~1.5.11",
    "angular-material": "^1.1.3",
    "angular-ui-router": "^1.0.0-rc.1",
    "browser-sync": "^2.18.8",
    "in-publish": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.2",
    "karma-typescript-preprocessor2": "^1.2.1",
    "npm-run-all": "^4.0.1",
    "rimraf": "^2.5.4",
    "systemjs": "^0.20.7",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6"
  }
}
