{
  "name": "ng-esm",
  "version": "1.3.7",
  "description": "ES2015 modules and decorators with Angular 1",
  "main": "dist/main.js",
  "jsnext:main": "dist/esm/main.js",
  "typings": "dist/esm/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run link && run-p watch:* serve",
    "serve": "browser-sync start -s . -f dist example --startPath /example",
    "link": "node scripts/symlink.js",
    "test": "echo \"No tests yet...\" && exit 0",
    "build": "run-p build:*",
    "build:cjs": "tsc -p src",
    "build:esm": "tsc -d -p src -m es2015 --outDir dist/esm",
    "watch:cjs": "tsc -w -p src",
    "watch:esm": "tsc -w -d -p src -m es2015 --outDir dist/esm",
    "watch:example": "tsc -w -p example",
    "prebuild": "rimraf dist",
    "prepublish": "in-publish && npm run build || not-in-publish"
  },
  "keywords": [
    "angular",
    "ES2015",
    "ES6",
    "decorators"
  ],
  "author": "Christian Svensson <csvn.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/csvn/ng-esm.git"
  },
  "peerDependencies": {
    "angular": "^1.5.8",
    "angular-ui-router": "^1.0.0-beta.3"
  },
  "devDependencies": {
    "@types/angular": "^1.5.20",
    "@types/angular-material": "^1.1.41",
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-material": "^1.1.1",
    "angular-ui-router": "^1.0.0-beta.3",
    "browser-sync": "^2.18.1",
    "in-publish": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-typescript-preprocessor2": "^1.2.1",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "systemjs": "^0.19.41",
    "tslint": "^3.15.1",
    "typescript": "^2.1.1"
  }
}
